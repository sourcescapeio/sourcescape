{
  "name": "sourcescape",
  "version": "1.0.4",
  "author": "Jieren Chen @gukjoon",
  "bin": {
    "sourcescape": "bin/sourcescape"
  },
  "bugs": "https://github.com/sourcescapeio/sourcescape/issues",
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",    
    "axios": "^0.27.2",
    "cli-progress": "^3.11.2",
    "cli-table": "^0.3.11",
    "cross-fetch": "^3.1.5",
    "docker-cli-js": "^2.8.0",
    "dockerode": "^3.2.1",
    "find-process": "^1.4.4",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "open": "^7.3.1",
    "ts-node": "^8",
    "tslib": "^1",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/cli-table": "^0.3.0",
    "@types/dockerode": "^3.2.2",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^12",
    "@types/ws": "^8.5.3",
    "globby": "^10",
    "oclif": "^3",
    "shx": "^0.3.3",
    "typescript": "^4.1.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/config",
    "/lib",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sourcescapeio/sourcescape",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sourcescape",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sourcescapeio/sourcescape",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
